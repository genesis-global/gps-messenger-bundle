{
    "name": "genesis-global/gps-messenger-bundle",
    "type": "symfony-bundle",
    "description": "Fork of petitpress/gps-messenger-bundle with support for PHP 7.3",
    "keywords": ["messenger", "symfony", "google", "pubsub"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ronald Marfoldi",
            "email": "ronald.marfoldi@petitpress.sk"
        },
        {
            "name": "Petit Press, a.s.",
            "homepage": "https://www.petitpress.sk"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "symfony/messenger": "^4.4|^5.1",
        "symfony/options-resolver": "^4.4|^5.1",
        "google/cloud-pubsub": "^1.29"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "phpspec/prophecy-phpunit": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpstan/phpstan": "^0.12.64"
    },
    "autoload": {
        "psr-4": { "PetitPress\\GpsMessengerBundle\\": "" }
    }
}
